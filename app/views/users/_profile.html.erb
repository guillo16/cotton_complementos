<div class="profile-user-container">
  <h3 class= "product-user-title">TUS DATOS</h3>
  <h2>Nombre legal</h2>
  <p><%= @user.name %> <%= @user.last_name %></p>
  <h2>Fecha de nacimiento</h2>
  <p></p>
  <h2>Telefono</h2>
  <p><%= @user.phone %></p>
  <h2>Direción</h2>
  <p><%= @user.address %></p>
  <h2>Provincia</h2>
  <p><%= @user.state %></p>
  <h2>Localidad</h2>
  <p><%= @user.city %></p>
  <h2>Codigo postal</h2>
  <p><%= @user.postal %></p>
  <h2>Dirección de correo electrónico</h2>
  <p><%= @user.email %></p>
  <%= link_to 'Editar Perfil', edit_user_registration_path, class: "btn btn-gradient btn-lg btn-block" %>
</div>
