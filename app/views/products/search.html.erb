<%= render 'shared/main_navbar' %>
<div class= "product-search-wrapper">
  <p><strong>Resultados</strong> (<%= @products.uniq(&:title).count %>)</p>

  <div class= "product-container searcheable">
    <% @products.uniq(&:title).each do |product| %>
    <%= render 'shared/product_card', product: product %>
    <% end %>
  </div>
</div>
